
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>
<hr />
<form method="post" action="/Account/Login">
    <label>Enter username:</label><br />
    <input type="text" name="username" /><br />

    <label>Enter password:</label><br />
    <input type="password" name="password" /><br />

    <button>Login</button>
</form>

